from itertools import *

table = '13 18 25 28 31 34 36 43 46 52 57 63 64 67 75 76 78 81 82 87'
graph = 'ae ah bd be bg ch cf db de ea ed eb fc fg gb gf gh ha hc hg'

for per in permutations('abcdefgh'):
    new_graph = table
    for i in range(1, 9):
        new_graph = new_graph.replace(str(i), per[i-1])
    if set(graph.split()) == set(new_graph.split()):
        print(*per)
